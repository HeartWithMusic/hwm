<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
	"http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
	
<tiles-definitions>
	<!-- 前台模板 -->
	<definition name="template_front" template="/jsp/front/common/main.jsp">
		<put-attribute name="top" value="/jsp/front/common/top.jsp" />
		<put-attribute name="bottom" value="/jsp/front/common/bottom.jsp" />
		<put-attribute name="content" value="" />
	</definition>
	
	<!-- 主界面 -->	
	<definition name="showHome" extends="template_front">
		<put-attribute name="content" value="/index.jsp" />
	</definition>
</tiles-definitions>